{
  "name": "multi-zones-next-js",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "env-cmd turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "scripty": "^2.1.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.6.2"
}